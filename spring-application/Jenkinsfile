#!groovy

pipeline {
  agent none
  stages {
    stage('Maven Install') {
     agent {
      docker {
       image 'maven:3.5.0'
      }
     }
     steps {
       sh 'mvn compile'
     }
    }
	stage('Run spring-boot ') {
     agent {
      docker {
       image 'maven:3.5.0'
      }
     }
     steps {
       sh 'mvn spring-boot:run'
     }
    }
	
   }
  }
